[

  {
    "url": "/assets/*",
    "static": "assets/*"
  },

  {
    "url": "/",
    "method": ["get"],
    "controller": {
      "file": "sliderController",
      "function": "createElement"
    }
  },

  {
    "url": "/index",
    "method": ["GET"],
    "controller": {
      "file": "sliderController",
      "function": "createElement"
    }
  },

  {
    "url": "/produkte",
    "method": ["get"],
    "controller": {
          "file": "pagecontroller",
          "function": "loadDataProd"
        },
        "params" : {
          "get": ["search"]
        }
  },

  {
    "url": "/impressum",
    "method": ["get"],
    "page": "impressum"
  },

  {
    "url": "/details",
    "method": ["get"],
    "controller": {
      "file": "pagecontroller",
      "function": "loadDataDet"
    },
    "params" : {
      "get": ["dts"]
    }
  },

  {
    "url": "/cart",
    "page": "warenkorb"
   
  },

  {
    "url": "/fillToCart",
    "method": ["POST"],
    "controller": {
      "file": "warenkorbController",
      "function": "fill"
    }
   
  },

  {
    "url": "/newCart",
    "method": ["POST"],
    "controller": {
      "file": "warenkorbController",
      "function": "deleteProd"
    }
   
  },

  {
    "url": "/bestellen",
    "method": ["get"],
    "page": "order"
  },

  {
    "url": "/kaufen",
    "method": ["POST"],
    "controller": {
      "file": "orderController",
      "function": "completeOrder"
    }
  },

  {
    "url": "/rating",
    "method": ["POST"],
    "controller": {
      "file": "ratingController",
      "function": "handleRating"
    }
  },

  {
    "url": "/print",
    "method": ["GET"],
    "controller": {
      "file": "printController",
      "function": "handlePrintView"
    },
    "params" : {
      "get": ["id"]
    }
  },

  {"url": "/prodError",
   "method": ["GET"],
   "controller": {
      "file": "sliderController",
      "function": "createElementProdErr"
    }
  },

  {
    "url": "/fragment",
    "method": ["POST"],
    "controller": {
      "file": "warenkorbController",
      "function": "loadCart"
    }
   
  },

  {
    "url": "/reload",
    "method": ["POST"],
    "controller": {
      "file": "warenkorbController",
      "function": "reloadCart"
    }
   
  },

  {
    "url": "/*",
    "method": ["GET"],
    "controller": {
      "file": "sliderController",
      "function": "createElementSiteErr"
    }

  }


]
